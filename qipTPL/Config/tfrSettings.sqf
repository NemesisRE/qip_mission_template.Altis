// RADIO STRUCTURE
// Set the Teamspeak channel name
tf_radio_channel_name = "Mission";
// Whether long range radios are automatically added to group leaders
TFAR_giveLongRangeRadioToGroupLeaders = false;
// Should a side use the same short-wave frequencies
TFAR_SameSRFrequenciesForSide = true;
// Should a side use the same long-wave frequencies
TFAR_SameSRFrequenciesForSide = true;
// Should a side use the same underwater frequencies
TFAR_SameDDFrequenciesForSide = true;
// Give every Soldier a Microdagr to set the Freq on the 2K radios
TFAR_giveMicroDagrToSoldier = false;
// Give all Soldiers a 5K radio
TFAR_givePersonalRadioToRegularSoldier = true;
// A coefficient defining the level of radio signal interruption caused by terrain.
TF_terrain_interception_coefficient = 7.0;
// Default Backpack Radios
TFAR_DefaultRadio_Backpack_West = "tf_rt1523g_big";
TFAR_DefaultRadio_Backpack_East = "tf_anprc155";
TFAR_DefaultRadio_Backpack_Independent = "tf_mr3000";